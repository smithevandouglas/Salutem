<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salutem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Dinner"
            android:label="@string/dinner"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Lunch"
            android:label="@string/lunch"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Breakfast"
            android:label="@string/breakfast"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Mental"
            android:label="@string/mental_health"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Diet"
            android:label="@string/diet"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Community"
            android:label="@string/community"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Profile"
            android:label="@string/profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:label="@string/login_button"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sub_biking"
            android:label="@string/biking"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sub_exercise"
            android:label="@string/exercise"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sub_running"
            android:label="@string/running"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Exercise"
            android:label="@string/title_activity_exercise"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>